{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "inputs": ["**/*.tsx", "**/*.ts"],
      "outputs": ["dist/**", ".next/**"],
      "persistent": true
    }
  }
}
